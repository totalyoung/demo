            <!--                <artifactId>maven-war-plugin</artifactId>-->
            <!--                <configuration>-->
            <!--                    &lt;!&ndash;<includeEmptyDirectories>true</includeEmptyDirectories>&ndash;&gt;-->
            <!--                </configuration>-->
